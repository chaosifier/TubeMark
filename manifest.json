{
  "manifest_version": 2,
  "name": "TubeMark",
  "description": "Bookmar youtube timeslice.",
  "version": "1",
  "author": "Sagar Dahal",
  "browser_action": {
    "default_icon": "ic_launcher.png",
    "default_title": "WeTube"
  },
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "256": "ic_launcher.png"
  },
  "web_accessible_resources": ["mainPage.html", "videoInfo.js"]
}
